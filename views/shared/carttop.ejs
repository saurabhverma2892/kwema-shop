<div class="uk-width-medium-3-5 uk-text-left main-cart-details-list uk-flex uk-flex-nowrap">
    <div class="cart-product-image inline-block inline-block-top" style="background-image: url('<%=cartItem.productDetails[0].primaryImage%>');"></div>
    <div class="inline-block inline-block-top main-cart-details-list-details">
        <p><b><%=cartItem.productDetails[0].name%>: <%=cartItem.productDetails[0].details%></b></p>
        <p><b><%=language.ship%></b> <%=language.tenfifdays%> | <%=language.shipping%> <span class="pink-color"><%=language.mexicousd%></span> | <span class="pink-color"><%=language.restoftheworld%></span> <%=language.wewillcontact%></p>
    </div>
</div>
<div class="uk-width-medium-2-5 uk-text-right main-cart-right-details" style="font-size: 18px;">
   
    <div style="margin-bottom: 8px;">
        <small> Bracelet: </small><b class="pink-color">$<%=cartItem.productDetails[0].design.currencies[0].value%> <%=language.currencyCode%></b>
    </div>
    <div>
        <small>Quantity: 1</small>
    </div>
</div>